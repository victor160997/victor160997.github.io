ate.ativo === true && pomodoro.ativo === false) {\n      this.setState({ ativo: false });\n    }\n  }\n\n  handleChange(event) {\n    const key = event.target.name;\n    this.setState({ [key]: event.target.value })\n  }\n\n  handleClickInfo() {\n    const { timeWork, pausaCurta, pausaLonga, ciclos } = this.state;\n    const { getInfoTimeProps } = this.props;\n    if (Number(timeWork) < 25 || Number(timeWork) > 50) {\n      return alert('É recomendado que o tempo de foco seja de 25 a 50 minutos!')\n    } else if (Number(pausaCurta) < 5 || Number(pausaCurta) > 10){\n      return alert('É recomendado que o tempo de pausa curta seja de 5 a 10 minutos!')\n    } else if (Number(pausaLonga) < 20 || Number(pausaLonga) > 45){\n      return alert('É recomendado que o tempo de pausa longa seja de 20 a 45 minutos!')\n    } else if (Number(ciclos) < 2 || Number(ciclos) > 8){\n      return alert('É recomendado que seja feito de 2 a 8 ciclos!')\n    }\n    getInfoTimeProps({ timeWork, pausaCurta, pausaLonga, ciclos, ativo: true });\n    this.setState({ ativo: true })\n  }\n\n  handleInterruption() {\n    const { getInfoTimeProps } = this.props;\n    this.setState({\n      timeWork: 25,\n      pausaCurta: 5,\n      pausaLonga: 30,\n      ciclos: 8,\n      ativo: false,\n      selectMusic: '',\n    });\n    getInfoTimeProps({ timeWork: 25,\n      pausaCurta: 5,\n      pausaLonga: 30,\n      ciclos: 8,\n      ativo: false, });\n  }\n\n  render() {\n    const { timeWork, pausaCurta, pausaLonga, ciclos, ativo, selectMusic } = this.state;\n    const { reset } = this.props;\n    return (\n      <form className=\"form\">\n        <div className=\"inputs-form\">\n          <label htmlFor=\"work\">\n            Pomodoro\n            <input \n              value={ timeWork }\n              type=\"number\"\n              id=\"work\"\n              name=\"timeWork\" \n              onChange={ this.handleChange }\n            />\n          </label>\n          <label htmlFor=\"pausa\">\n            Pausa curta\n            <input\n              value={ pausaCurta } \n              type=\"number\"\n              id=\"pausaCurta\"\n              name=\"pausaCurta\"\n              onChange={ this.handleChange } \n            />\n          </label>\n          <label htmlFor=\"pausa\">\n            Pausa longa\n            <input\n              value={ pausaLonga } \n              type=\"number\"\n              id=\"pausaLonga\"\n              name=\"pausaLonga\"\n              onChange={ this.handleChange } \n            />\n          </label>\n          <label htmlFor=\"ciclos\">\n            Ciclos\n            <input\n              value={ ciclos } \n              type=\"number\"\n              id=\"ciclos\"\n              name=\"ciclos\"\n              onChange={ this.handleChange } \n            />\n          </label>\n        </div>\n        <div className=\"buttons-form\">\n          <button \n            type=\"button\"\n            onClick={ this.handleClickInfo }\n            disabled ={ ativo ? true : false }\n          >\n            Começar\n          </button>\n          <button\n            type=\"button\"\n            onClick={ this.handleInterruption }\n            disabled ={ ativo ? false : true }\n          >\n            Reset\n          </button>\n        </div>\n        <label htmlFor=\"selectMusic\">\n          Selecione uma música para se concentrar melhor:\n          <select\n            className=\"select-form\" \n            name=\"selectMusic\" \n            id=\"selectMusic\"\n            value={ selectMusic }\n            onChange={ this.handleChange }\n          >\n            <option value=\"nenhuma\">Nenhuma</option>\n            <option value=\"Concentration\">Concentration</option>\n            <option value=\"jazz\">Jazz</option>\n            <option value=\"opera\">Opera</option>\n          </select>\n        </label>\n        <div className=\"yt-videos\">\n          { selectMusic==='Concentration' ? <YoutubeConcentration /> : '' }\n          { selectMusic==='jazz' ? <YoutubeJazz /> : '' }\n          { selectMusic==='opera' ? <YoutubeOpera /> : '' }\n        </div>\n      </form>\n    )\n  }\n}\n\nForm.propTypes = {\n  getInfoTimeProps: PropTypes.func.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  pomodoro: state.time.time,\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n  getInfoTimeProps: (payload) => dispatch(getInfoTimeAction(payload)),\n  reset: (payload) => dispatch(resetAction(payload)),\n})\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Form)\n","/home/victor/Área de Trabalho/my-pomodoro.app/src/components/Cronometro.jsx",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/PlaySound.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/HomePage.jsx",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/store/store.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/reducers/index.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/reducers/time.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/actions/index.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/PlayRap.js",[],["72","73"],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/PlayJazz.js",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/YoutubeJazz.jsx",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/YoutubeConcentration.jsx",[],"/home/victor/Área de Trabalho/my-pomodoro.app/src/components/YoutubeOpera.jsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":72,"column":13,"nodeType":"80","messageId":"81","endLine":72,"endColumn":18},{"ruleId":"74","replacedBy":"82"},{"ruleId":"76","replacedBy":"83"},"no-native-reassign",["84"],"no-negated-in-lhs",["85"],"no-unused-vars","'reset' is assigned a value but never used.","Identifier","unusedVar",["84"],["85"],"no-global-assign","no-unsafe-negation"]